<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Patients</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>Speciality</h1>
    <table>
        <thead>
            <tr>
                <th>Provider ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Provider Speciality</th>
                <th>Email Adress</th>
                <th>Phone Number</th>
                <th>Date Joined</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(providers => { %>
                <tr>
                    <td><%= providers.provider_id %></td>
                    <td><%= providers.first_name  %></td>
                    <td><%= providers.last_name  %></td>
                    <td><%= providers.provider_specialty  %></td>
                    <td><%= providers.email_address  %></td>
                    <td><%= providers.phone_number  %></td>
                    <td><%= providers.date_joined  %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>